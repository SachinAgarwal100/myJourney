<NamedCredential xmlns="http://soap.sforce.com/2006/04/metadata">
    <allowMergeFieldsInBody>false</allowMergeFieldsInBody>
    <allowMergeFieldsInHeader>false</allowMergeFieldsInHeader>
    <authProvider>Google_Auth_Provider</authProvider>
    <endpoint>https://www.googleapis.com/calendar/v3</endpoint>
    <generateAuthorizationHeader>true</generateAuthorizationHeader>
    <label>Google Named Cred</label>
    <oauthScope>openid https://www.googleapis.com/auth/userinfo.profile https://www.googleapis.com/auth/calendar https://www.googleapis.com/auth/calendar.calendarlist https://www.googleapis.com/auth/userinfo.profile https://www.googleapis.com/auth/calendar.events.owned</oauthScope>
    <principalType>NamedUser</principalType>
    <protocol>Oauth</protocol>
</NamedCredential>
